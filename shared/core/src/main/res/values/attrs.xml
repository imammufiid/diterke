<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TransportCardView">
        <attr name="image" format="reference"/>
        <attr name="title" format="string"/>
        <attr name="isTransportSelected" format="boolean"/>
        <attr name="transportType" format="enum">
            <enum name="bike" value="0"/>
            <enum name="car" value="1"/>
            <enum name="taxi" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ButtonView">
        <attr name="buttonTitle" format="string"/>
        <attr name="buttonType" format="enum">
            <enum name="base" value="0"/>
            <enum name="facebook" value="1"/>
            <enum name="google" value="2"/>
        </attr>
    </declare-styleable>
</resources>